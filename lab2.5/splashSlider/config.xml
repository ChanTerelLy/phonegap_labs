<?xml version='1.0' encoding='utf-8'?>
<widget id="test.test.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>blank_test</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~5.0.3" />
    <preference name="SplashScreenDelay" value="10000" />
    <platform name="browser">
        <preference name="SplashScreen" value="res/splash.jpg" /> <!-- defaults to "/img/logo.png" -->
<!--        <preference name="AutoHideSplashScreen" value="false" /> &lt;!&ndash; defaults to "true" &ndash;&gt;-->
        <preference name="SplashScreenDelay" value="3000" /> <!-- defaults to "3000" -->
        <preference name="SplashScreenBackgroundColor" value="green" /> <!-- defaults to "#464646" -->
<!--        <preference name="ShowSplashScreen" value="true" /> &lt;!&ndash; defaults to "true" &ndash;&gt;-->
<!--        <preference name="SplashScreenWidth" value="600" /> &lt;!&ndash; defaults to "170" &ndash;&gt;-->
<!--        <preference name="SplashScreenHeight" value="300" /> &lt;!&ndash; defaults to "200" &ndash;&gt;-->
    </platform>

    <platform name="android">
        <preference name="SplashScreen" value="res/splash.jpg" /> <!-- defaults to "/img/logo.png" -->
        <preference name="SplashScreenDelay" value="3000" /> <!-- defaults to "3000" -->
        <preference name="SplashScreenBackgroundColor" value="green" />
    </platform>
    <access origin="*" />
</widget>
